{
  "workbench.iconTheme": "material-icon-theme",
  "todo-tree.tree.showScanModeButton": false,
  "editor.formatOnSave": true,
  "typescript.format.enable": false,
  "emmet.triggerExpansionOnTab": true,
  "editor.quickSuggestions": true,
  "json.format.enable": false,
  "javascript.format.enable": false,
  "files.autoSave": "onFocusChange",
  "javascript.validate.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "files.trimTrailingWhitespace": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.tree.indent": 16,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.detectIndentation": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.host": "",
  "liveServer.settings.useLocalIp": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "sync.gist": "ac5c9dfef6edb7320dfe444522444de4",
  "workbench.startupEditor": "newUntitledFile",
  "editor.tabSize": 2,
  "colorize.languages": [
    "typescript",
    "javascript",
    "css",
    "scss",
    "javascriptreact",
    "typescriptreact",
    "html",
    "html5"
  ],
  "terminal.integrated.shell.osx": "/bin/zsh",
  "indentRainbow.colors": [
    "rgba(247, 37, 133,0.1)",
    "rgba(181, 23, 158,0.1)",
    "rgba(114, 9, 183, 0.1)",
    "rgba(86, 11, 173,0.1)",
    "rgba(72, 12, 168,0.1)",
    "rgba(58, 12, 163,0.1)",
    "rgba(63, 55, 201,0.1)",
    "rgba(67, 97, 238,0.1)",
    "rgba(72, 149, 239,0.1)",
    "rgba(76, 201, 240,0.1)"
  ],
  "editor.smoothScrolling": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "colorize.colorized_colors": [
    "BROWSERS_COLORS",
    "HEXA",
    "RGB",
    "HSL",
    "BROWSERS_COLORS"
  ],
  "files.eol": "\n",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "turboConsoleLog.quote": "'",
  "turboConsoleLog.delimiterInsideMessage": "=====",
  "cSpell.userWords": ["UNPROCESSABLE"],
  "typescript.validate.enable": false,
  "typescript.suggestionActions.enabled": false,
  "git.autofetch": true,
  "editor.renderWhitespace": "all",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "edge": "html",
    "postcss": "css"
  },
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "explorer.openEditors.visible": 10,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 200,
  "editor.minimap.showSlider": "always",
  "editor.cursorSmoothCaretAnimation": true,
  "files.insertFinalNewline": true,
  "editor.linkedEditing": true,
  "editor.maxTokenizationLineLength": 999999999999,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "html.suggest.html5": true,
  "editor.fontFamily": "JetBrains Mono, Menlo, SF Mono",
  "cSpell.language": "en,vi",
  //   "terminal.integrated.fontFamily": "FiraCode Nerd Font Retina",
  "redhat.telemetry.enabled": false,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.insertEnclosingFunction": false,
  "scss.lint.unknownAtRules": "ignore",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "editor.lineHeight": 28,
  "editor.letterSpacing": 0.5,
  "tailwindCSS.emmetCompletions": true,
  "powermode.presets": "magic",
  "editor.suggest.preview": true,
  "editor.fontSize": 14,
  "workbench.colorTheme": "One Dark Pro Flat",
  "window.zoomLevel": -1,
  "editor.fontLigatures": false
  // "editor.guides.bracketPairs": true
  // "javascript.inlayHints.parameterNames.enabled": "all",
  // "typescript.inlayHints.parameterNames.enabled": "all"
}
